<% if (!_.isEmpty(cohorts)) { %>
<hr>
<label for="cohort-filter">
    <%- selectDisplayName %>
</label>
<select id="cohort-filter" class="form-control">
    <% _.each(cohorts, function (cohort) { %>
        <option value="<%- cohort.cohortName %>" <% if (cohort.selected) { %> selected <% } %>>
            <%- cohort.displayName %>
        </option>
    <% }); %>
</select>
<% } %>
